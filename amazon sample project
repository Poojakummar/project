package Github.demo;

import io.restassured.path.json.JsonPath;

public class JsonParsing {
	
  public void name () {
	  JsonPath js=new JsonPath (Payload.employeeinfo());
	  int noOfEmployees = js.getInt("employeeDetails.size()");
			  System.out.println(noOfEmployees);
			  
			  int tsalary=js.getInt("dashBoard.totalSlary");
			  System.out.println(tsalary);
			  
			  Object name =js.getInt("employeeDetails.name[0]");
			  System.out.println(name);
			  
			  Object exp = js.getInt("employeeDetails.name[1]");
			  System.out.println(exp);
			  
			  
			  
			  
  }
	
}
